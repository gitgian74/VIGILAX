{
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@3.0.0"
    },
    "api/mcp.py": {
      "runtime": "@vercel/python@3.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/mcp",
      "destination": "/api/mcp.py"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    }
  ]
}